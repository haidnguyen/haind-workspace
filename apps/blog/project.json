{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/blog",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/blog",
        "outputPath": "dist/apps/blog"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "blog:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "blog:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "blog:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "blog:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/blog"],
      "options": {
        "jestConfig": "apps/blog/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/blog/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start dist/apps/blog"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vercel --prod --prebuilt"
      }
    },
    "build-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vercel build --prod"
      }
    }
  },
  "tags": []
}
